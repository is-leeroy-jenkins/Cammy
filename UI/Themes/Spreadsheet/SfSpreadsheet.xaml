                                                Height="30"
                                                Margin="0,0,0,0"
                                                Orientation="Horizontal">
                                                <TextBlock
                                                    x:Name="data"
                                                    Margin="3,8,3,0"
                                                    Foreground="{StaticResource ContentForegroundAlt1}"
                                                    IsEnabled="False"
                                                    Text="{spreadsheet:SpreadsheetLocalizationResourceExtension ResourceName=Datatitle}" />
