                                                    x:Name="InCellDropDown"
                                                    Width="112"
                                                    Height="20"
                                                    Content="{spreadsheet:SpreadsheetLocalizationResourceExtension ResourceName=InCellDropDown}"
                                                    IsChecked="True"
                                                    TabIndex="6"
                                                    Visibility="Collapsed">
                                                    <CheckBox.Margin>
                                                        <Thickness>180.5,0,0,10</Thickness>
                                                    </CheckBox.Margin>
                                                </CheckBox>
                                            </StackPanel>
                                            <ComboBox
                                                x:Name="ComparisonOperatorCombo"
                                                Width="200"
                                                Height="24"
                                                Margin="3,5,3,3"
                                                HorizontalAlignment="left"
                                                DisplayMemberPath="DV_ComparisionOperatorName"
                                                ItemsSource="{Binding Path=DataList, ElementName=validationWindow, Mode=TwoWay}"
                                                SelectedValuePath="DV_ComparisionOperator"
                                                Style="{StaticResource SpreadsheetComboBoxStyle}"
                                                TabIndex="2" />
                                            <TextBlock
                                                x:Name="firstValueTxtBlock"
                                                Margin="3,8,3,0"
                                                Foreground="{StaticResource ContentForegroundAlt1}" />
                                            <TextBox
                                                x:Name="firstValueTxtBox"
                                                Width="266"
                                                Height="24"
                                                Margin="3,8,3,3"
                                                HorizontalAlignment="Left"
                                                Style="{StaticResource WPFTextBoxStyle}"
                                                TabIndex="3"
                                                Visibility="{Binding ElementName=allowCombo, Path=SelectedValue, Converter={StaticResource MinValueConverter}}" />
                                            <TextBlock
                                                x:Name="secondValueTxtBlock"
                                                Margin="3,8,3,0"
                                                Foreground="{StaticResource ContentForegroundAlt1}" />
                                            <TextBox
                                                x:Name="secondValueTxtBox"
                                                Width="266"
                                                Height="24"
                                                Margin="3,8,3,3"
                                                HorizontalAlignment="Left"
                                                IsEnabled="{Binding ElementName=allowCombo, Path=SelectedValue, Converter={StaticResource StingToBoolConverter}}"
                                                Style="{StaticResource WPFTextBoxStyle}"
                                                TabIndex="4"
                                                Visibility="{Binding ElementName=dataCombo, Path=SelectedValue, Converter={StaticResource MaxValueConverter}}" />
                                        </StackPanel>
                                    </Grid>
                                </tools:TabItemExt>
                                <tools:TabItemExt
                                    Width="109"
                                    Height="24"
                                    Header="{spreadsheet:SpreadsheetLocalizationResourceExtension ResourceName=InputMessage}">
                                    <Grid
                                        Height="284"
                                        Margin="12,6,0,0">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="85*" />
                                            <ColumnDefinition Width="109*" />
                                        </Grid.ColumnDefinitions>
