                                                    Width="200"
                                                    Height="24"
                                                    Margin="3"
                                                    DisplayMemberPath="DV_ExcelDataTypeName"
                                                    ItemsSource="{Binding Path=AllowTypeList, ElementName=validationWindow, Mode=TwoWay}"
                                                    SelectedValuePath="DV_ExcelDataType"
                                                    Style="{StaticResource SpreadsheetComboBoxStyle}"
                                                    TabIndex="1" />
