    <Style
        x:Key="FindAndReplaceDialog"
        TargetType="{x:Type spreadsheet:FindAndReplaceDialog}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type spreadsheet:FindAndReplaceDialog}">
                    <Grid
                        Margin="0,0,0,0"
                        Background="{StaticResource ContentBackground}">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="*" />
                            <RowDefinition Height="Auto" />
                        </Grid.RowDefinitions>
                        <tools:TabControlExt
                            x:Name="tabcontrol"
                            Margin="0,0,0,4"
                            AllowDragDrop="False"
                            Background="{StaticResource ContentBackground}"
                            BorderBrush="{StaticResource BorderAlt}"
                            CloseButtonType="Hide"
                            EnableLabelEdit="False"
                            SelectedItemFontWeight="Medium"
                            ShowTabItemContextMenu="False"
                            TabListContextMenuOptions="None"
                            TabPanelBackground="{StaticResource ContentBackground}">
                            <tools:TabControlExt.BorderThickness>
                                <Thickness>0,1,0,0</Thickness>
                            </tools:TabControlExt.BorderThickness>
                            <tools:TabItemExt
                                Width="54"
                                Height="24"
                                Header="{spreadsheet:SpreadsheetLocalizationResourceExtension ResourceName=Find_Find}">
                                <tools:TabItemExt.Margin>
                                    <Thickness>12,0,0,0</Thickness>
                                </tools:TabItemExt.Margin>
                                <StackPanel Margin="13,0,5,0">
                                    <Grid Margin="0,11,0,0">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="60" />
                                            <ColumnDefinition Width="*" />
                                        </Grid.ColumnDefinitions>
                                        <TextBlock
                                            Padding="0,3,0,0"
                                            Foreground="{StaticResource ContentForegroundAlt1}"
                                            Text="{spreadsheet:SpreadsheetLocalizationResourceExtension ResourceName=Find_FindWhat}">
                                            <TextBlock.Margin>
                                                <Thickness>0,5,0,0</Thickness>
                                            </TextBlock.Margin>
                                        </TextBlock>
                                        <ComboBox
                                            x:Name="FindSearchComboBox"
                                            Grid.Column="1"
                                            Height="24"
                                            MinWidth="344"
                                            Margin="15,4,8,0"
                                            HorizontalAlignment="Stretch"
                                            IsEditable="True"
                                            IsTextSearchEnabled="False" />
                                    </Grid>
                                    <Grid
                                        x:Name="FindOuterGrid"
                                        Margin="0,41,0,11">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="*" />
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto" />
                                        </Grid.RowDefinitions>
                                        <Grid
                                            x:Name="FindInnerGrid"
                                            Margin="0,0,0,0"
                                            HorizontalAlignment="Left"
                                            Visibility="Collapsed">
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="30" />
                                                <RowDefinition Height="34" />
                                                <RowDefinition Height="30" />
                                            </Grid.RowDefinitions>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="70" />
                                                <ColumnDefinition Width="Auto" />
                                            </Grid.ColumnDefinitions>
                                            <TextBlock
                                                Foreground="{StaticResource ContentForegroundAlt1}"
                                                Text="{spreadsheet:SpreadsheetLocalizationResourceExtension ResourceName=Find_Within}">
                                                <TextBlock.Padding>
                                                    <Thickness>0,7,0,0</Thickness>
                                                </TextBlock.Padding>
                                            </TextBlock>
                                            <ComboBox
                                                x:Name="FindSearchWithinComboBox"
                                                Grid.Row="0"
                                                Grid.Column="1"
                                                Width="133"
                                                Height="24"
                                                Margin="5,0,0,0"
                                                Style="{StaticResource SpreadsheetComboBoxStyle}">
                                                <ComboBoxItem Content="{spreadsheet:SpreadsheetLocalizationResourceExtension ResourceName=Sheet}" />
                                                <ComboBoxItem Content="{spreadsheet:SpreadsheetLocalizationResourceExtension ResourceName=Find_Workbook}" />
                                            </ComboBox>
                                            <TextBlock
                                                Grid.Row="1"
                                                Grid.Column="0"
                                                Foreground="{StaticResource ContentForegroundAlt1}"
                                                Text="{spreadsheet:SpreadsheetLocalizationResourceExtension ResourceName=Find_Search}">
                                                <TextBlock.Margin>
                                                    <Thickness>0,9,0,0</Thickness>
                                                </TextBlock.Margin>
                                            </TextBlock>
                                            <ComboBox
                                                x:Name="FindSearchByComboBox"
                                                Grid.Row="1"
                                                Grid.Column="1"
                                                Width="133"
                                                Height="24"
                                                Margin="5,0,0,0"
                                                Style="{StaticResource SpreadsheetComboBoxStyle}">
                                                <ComboBoxItem Content="{spreadsheet:SpreadsheetLocalizationResourceExtension ResourceName=Find_ByRows}" />
                                                <ComboBoxItem Content="{spreadsheet:SpreadsheetLocalizationResourceExtension ResourceName=Find_ByColumns}" />
                                            </ComboBox>
                                            <TextBlock
                                                Grid.Row="2"
                                                Grid.Column="0"
                                                Foreground="{StaticResource ContentForegroundAlt1}"
                                                Text="{spreadsheet:SpreadsheetLocalizationResourceExtension ResourceName=Find_Lookin}">
                                                <TextBlock.Padding>
                                                    <Thickness>0,7,0,0</Thickness>
                                                </TextBlock.Padding>
                                            </TextBlock>
                                            <ComboBox
                                                x:Name="FindLookinComboBox"
                                                Grid.Row="2"
                                                Grid.Column="1"
                                                Width="133"
                                                Height="24"
                                                Margin="5,0,0,0"
                                                Style="{StaticResource SpreadsheetComboBoxStyle}">
                                                <ComboBoxItem Content="{spreadsheet:SpreadsheetLocalizationResourceExtension ResourceName=Find_RibbonFormulas}" />
                                                <ComboBoxItem Content="{spreadsheet:SpreadsheetLocalizationResourceExtension ResourceName=Context_PasteValues}" />
                                            </ComboBox>
                                        </Grid>
                                        <StackPanel
                                            x:Name="FindCheckBoxStackPanel"
                                            Grid.Row="0"
                                            Grid.Column="1"
